    "t=int(input())\n",
    "\n",
    "\n",
    "for i in range(t):\n",
    "    n=int(input())\n",
    "    if n==0:\n",
    "        print(\"0\")\n",
    "        continue\n",
    "    while n>0:\n",
    "        digit=n%10\n",
    "        print(digit,end=\" \")\n",
    "        n=n//10\n",
    "    print()    "
   ]
